{
  "name": "dmhendricks/carbon-fields-loader",
  "type": "wordpress-plugin",
  "description": "A plugin for WordPress that loads the latest Carbon Fields framework.",
  "keywords": ["wordpress", "plugin", "carbon fields", "custom", "fields", "loader"],
  "homepage": "https://github.com/dmhendricks/carbon-fields-loader/",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Daniel M. Hendricks",
      "homepage": "https://danhendricks.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/dmhendricks/carbon-fields-loader/issues/",
    "source": "https://github.com/dmhendricks/carbon-fields-loader/"
  },
  "require": {
    "php": ">=5.3.29",
    "htmlburger/carbon-fields": "^2.1.0",
    "composer/installers": "^1.3.0"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "TwoLabNet\\CarbonFieldsLoader\\": "app/"
    }
  },
  "scripts": {
      "pre-autoload-dump": [
        "find ./vendor -name '.git*' -exec rm -rf {} +"
      ]
  }
}
